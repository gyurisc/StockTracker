{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "api-route": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      },
      "client-route": {
        "ClusterId": "client-cluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "api-cluster": {
        "Destinations": {
          "api": {
            "Address": "http://localhost:5001"
          }
        }
      },
      "client-cluster": {
        "Destinations": {
          "client": {
            "Address": "http://localhost:5174"
          }
        }
      }
    }
  }
}